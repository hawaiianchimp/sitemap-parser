{
  "name": "sitemapper",
  "version": "3.0.0",
  "description": "Parser for XML Sitemaps to be used with Robots.txt and web crawlers",
  "keywords": [
    "parse",
    "sitemap",
    "xml",
    "robots.txt",
    "sitemaps",
    "crawlers",
    "webcrawler"
  ],
  "homepage": "http://github.com/hawaiianchimp/sitemapper",
  "tonicExampleFilename": "example.js",
  "bugs": {
    "url": "http://github.com/hawaiianchimp/sitemapper/issues"
  },
  "license": "MIT",
  "files": [
    "lib",
    "sitemapper.d.ts"
  ],
  "main": "./lib/assets/sitemapper.js",
  "types": "./sitemapper.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/hawaiianchimp/sitemapper.git"
  },
  "author": {
    "name": "Sean Thomas Burke",
    "email": "sean@seantburke.com",
    "url": "http://www.seantburke.com"
  },
  "scripts": {
    "compile": "babel src -d lib -s",
    "build": "npm run clean && npm run compile",
    "start": "node lib/examples/index.js",
    "test": "mocha ./lib/tests/test.js && npm run lint",
    "lint": "eslint /src",
    "clean": "rm -rf lib",
    "docs": "documentation build ./src/assets/sitemapper.js -f md > docs.md"
  },
  "maintainers": [
    {
      "name": "Sean Thomas Burke",
      "email": "sean@seantburke.com",
      "url": "http://www.seantburke.com"
    }
  ],
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "async": "^3.1.0",
    "documentation": "^8.1.2",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.12.0",
    "is-url": "^1.2.2",
    "mocha": "^5.2.0",
    "should": "^10.0.0"
  },
  "dependencies": {
    "es6-promise": "^3.3.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "xml2js-es6-promise": "^1.1.1"
  }
}
